  },
  "homepage": "https://github.com/RoleSave/dparse#readme",
  "devDependencies": {
    "@types/node": "^13.13.0",
    "gruetest": "^1.0.2",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  }
